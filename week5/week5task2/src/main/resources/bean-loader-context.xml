<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
                        http://www.springframework.org/schema/context
                        http://www.springframework.org/schema/context/spring-context-3.2.xsd http://www.springframework.org/schema/aop https://www.springframework.org/schema/aop/spring-aop.xsd">
    
    <bean id="user-by-constructor" class="springBean.User">
    	<property name="id" value="1" />
        <property name="name" value="构造方法装配" />
    </bean>
    
    <bean id="user-by-static-method" class="springBean.User" factory-method="createUserStatic"></bean>
    
    <bean id="user-by-factory-bean" class="springBean.UserFactoryBean"></bean>

</beans>